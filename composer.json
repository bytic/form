{
  "name": "bytic/form",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/ByTIC/form",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Form\\": "src"
    },
    "classmap": [
      "legacy",
      "src"
    ],
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Form\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "bytic/locale": "^0.9|^1.0",
    "symfony/form": "^4.0|^5.0",
    "ezyang/htmlpurifier": "^4.0"
  },
  "require-dev": {
    "bytic/view": "^0.9|^1.0",
    "bytic/google-recaptcha": "^1.0",
    "bytic/orm": "^0.9|^1.0",
    "bytic/money": "^0.9|^1.0",
    "bytic/controllers": "^0.9|^1.0",
    "bytic/phpqatools": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "bytic/phpqatools": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
